<div class="container">
  <div class="row">
    <h2>ユーザー一覧</h2>
    <div>
      <%= form_with url: search_path,method: :get, local: true do |f| %>
        <%= f.text_field :content %>
        <%= f.select :model,options_for_select({"User"=>"user","Give"=>"give","Take"=>"take"})%>
        <%= f.submit'検索' %>
      <% end %>
    </div>
      <%= render 'users/user', users: @users %>
  </div>
</div>